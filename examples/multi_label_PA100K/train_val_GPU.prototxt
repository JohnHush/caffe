 }
}

layer {
	name: "sigmoid"
	type: "Sigmoid"
	bottom: "fc8_PA100K_26"
	top: "sigmoid_fc8_PA100K_26"
	include: { phase: TEST }
}

layer {
  name: "mlaccuracy"
  type: "MultiLabelAccuracy"
  bottom: "sigmoid_fc8_PA100K_26"
  bottom: "label"
  top: "accuracy_whole"
  top: "accuracy_per_class"
  include: { phase: TEST }
}
