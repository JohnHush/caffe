    positive_ratio : 0.84
    positive_ratio : 0.14332
    positive_ratio : 0.3020
    positive_ratio : 0.3807
    positive_ratio : 0.3172
    positive_ratio : 0.0404
    positive_ratio : 0.1868
    positive_ratio : 0.1974
    positive_ratio : 0.2043
    positive_ratio : 0.2588
    positive_ratio : 0.0083
    positive_ratio : 0.561
    positive_ratio : 0.4387
    positive_ratio : 0.038
    positive_ratio : 0.1267
    positive_ratio : 0.100
    positive_ratio : 0.0254
    positive_ratio : 0.00199
    positive_ratio : 0.0097
    positive_ratio : 0.067
    positive_ratio : 0.734
